{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["overall_verdict", "overall_confidence", "summary", "gaps"],
  "properties": {
    "overall_verdict": {
      "type": "string",
      "enum": ["compliant", "non_compliant", "needs_review"]
    },
    "overall_confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "summary": {
      "type": "string",
      "minLength": 1
    },
    "gaps": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}
